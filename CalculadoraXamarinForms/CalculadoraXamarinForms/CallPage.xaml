<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="CalculadoraXamarinForms.CallPage">
    <ContentPage.Content>
        <StackLayout Padding="30">
            <Entry x:Name="EntryNumero" Placeholder="Número de teléfono" Keyboard="Telephone"/>
            <Button Text="Llamar" Clicked="OnLlamarClicked"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>